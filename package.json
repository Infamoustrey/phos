{
  "name": "phos",
  "version": "0.0.1",
  "description": "An Open Source alternative to softwares like Proclaim, OpenLP, & Pro-Presenter",
  "author": "Trey Foster <treybfoster@gmail.com@gmail.com>",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"yarn hot\" \"yarn electron\"",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "electron": "cross-env NODE_ENV=development electron main.js"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.11",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.7",
    "axios": "^0.19.0",
    "bible-gateway-api": "1.0.8",
    "dayjs": "^1.8.17",
    "electron": "^7.1.1",
    "electron-store": "^5.1.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-draft-wysiwyg": "^1.13.2",
    "react-lazy-load-image-component": "^1.3.2",
    "react-redux": "^7.1.3",
    "reactn": "^2.2.4",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.7.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "laravel-mix": "^5.0.0",
    "node-sass": "^4.13.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.0"
  },
  "license": "MIT"
}
